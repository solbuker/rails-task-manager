<div class='container'>
  <div class='box'>
      <h1>Your tasks</h1>
  <ul class = 'list'>
    <%@tasks.each do |task|%>
      <li><%= link_to task.title, task_path(task)%></li>
    <%end%>
  </ul>
  <div class='button'>
    <p class='buttontxt'>
      <%= link_to "Add new task", new_task_path %>
    </p>
  </div>
  </div>
</div>
